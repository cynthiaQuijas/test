    "def func1(decimal):\n",
    "    if decimal == 0:\n",
    "        return '0b0'\n",
    "    binario = ''\n",
    "    while decimal > 0:\n",
    "        binario = str(decimal % 2) + binario\n",
    "        decimal //= 2\n",
    "    return '0b' + binario\n"
   ]
